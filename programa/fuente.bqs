program pepe;

var 
  juan: float;
  pato: string;

{
  pepe := "casa";
  juan := 3;
  if [pato <= juan] & [pato > juan] {
    write(substr("test ", 1, 3));
  }
  else {
    read("hola ", pepe);
  };

  while ![pato < juan] | ![juan >= pepe] {
    write(pepe);
  };

  if ![juan = pepe] {

    juan := find("hola ", "h");
  };
  
  pepe := long("test2");
}
